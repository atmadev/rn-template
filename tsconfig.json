{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": [
			"es2019",
			"dom"
		],
		"jsx": "react-native",
		"noEmit": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": true,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"baseUrl": "./sources/",
		"paths": {
			"assets/*": [
				"../assets/*"
			],
			"shared/*": [
				"../shared/*"
			]
		}
	}
}